module Libc
{
    pub let Stdout usize ->
        LibcUnsafe:stdout;

    pub func Printf(pattern String, arg String)
    {
        LibcUnsafe:printf(pattern.ToByteArray(), arg.ToByteArray());
    }

    pub func Fputs(str String, stream usize)
    {
        LibcUnsafe:fputs(str.ToByteArray(), stream);
    }
}
