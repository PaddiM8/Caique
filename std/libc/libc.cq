class Libc
{
    static func Printf(pattern String, arg String)
    {
        LibcUnsafe:printf(pattern.ToByteArray(), arg.ToByteArray());
    }

    static func Fputs(str String, stream usize)
    {
        LibcUnsafe:fputs(str.ToByteArray(), stream);
    }

    static func GetStdout() usize
    {
        LibcUnsafe:GetStdout()
    }
}
